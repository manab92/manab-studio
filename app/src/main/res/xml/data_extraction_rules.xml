<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude app-specific data that shouldn't be backed up to cloud -->
        <exclude domain="sharedpref" path="device_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Include data that should be transferred to new device -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>